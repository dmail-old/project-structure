{
  "name": "@dmail/project-structure",
  "version": "2.0.0",
  "licence": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dmail/project-structure"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "module": "index.js",
  "files": [
    "index.js",
    "src",
    "dist"
  ],
  "scripts": {
    "lint": "eslint .",
    "compile": "node ./script/compile.js",
    "clean": "rimraf dist && rimraf coverage && rimraf .nyc_output",
    "dist": "npm run clean && npm run compile",
    "test": "npm run dist && node ./dist/index.test.js",
    "cover": "npm run dist && nyc node ./dist/index.test.js",
    "coverage-upload": "codecov",
    "prepublishOnly": "npm run dist",
    "reinstall": "npm run clean && rimraf node_modules && npm install"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/plugin-transform-spread": "7.0.0",
    "@dmail/assert": "1.0.0",
    "@dmail/cancellation": "github:dmail/cancellation#1.0",
    "@dmail/project-eslint-config": "github:dmail/project-eslint-config",
    "@dmail/project-prettier-config": "github:dmail/project-prettier-config",
    "babel-eslint": "9.0.0",
    "codecov": "2.3.0",
    "eslint": "5.1.0",
    "eslint-plugin-import": "2.6.0",
    "nyc": "11.2.1",
    "prettier": "1.15.3",
    "rimraf": "2.6.2",
    "rollup": "1.1.2",
    "rollup-plugin-babel": "4.3.2",
    "rollup-plugin-node-resolve": "4.0.0"
  }
}
